{
  "name": "study-gen",
  "version": "0.0.1",
  "description": "Frontend for researcher study generator",
  "scripts": {
    "deploy": "forever node dist/study-gen.js",
    "start": "nodemon --watch dist --ext js,css,html,png dist/study-gen.js",
    "flow": "flow",
    "build": "flow && webpack --config webpack.config.js  --progress --colors --bail",
    "build-client": "babel src --only='**assets/*' -d dist/public",
    "test": "mocha --compilers js:node_modules/babel-core/register --harmony -R spec -t 5000 --sort test/*.spec.js"
  },
  "main": "dist/study-gen.js",
  "author": "Anders Borch <anders@cyborch.com>",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.5.1",
    "array-includes": "^3.0.2",
    "babel-polyfill": "^6.20.0",
    "cookies-js": "^1.2.3",
    "dialog-polyfill": "^0.4.5",
    "es6-promisify": "^5.0.0",
    "getmdl-select": "^1.0.4",
    "koa": "^2.0.0",
    "koa-body": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.0.1",
    "koa-static-folder": "^0.1.6",
    "material-design-lite": "^1.2.1",
    "mocha": "^2.5.3",
    "node-uuid": "^1.4.7",
    "pug": "^2.0.0-beta6",
    "pug-plugin-ng": "0.0.2",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "url-search-params": "^0.6.1",
    "whatwg-fetch": "^2.0.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-tcomb": "^0.3.24",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "flow-bin": "^0.37.4",
    "forever": "^0.15.3",
    "jsdom": "^9.9.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "tcomb": "^3.2.15",
    "webpack": "^1.12.14"
  }
